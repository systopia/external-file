<?xml version="1.0"?>
<extension key="external-file" type="module">
  <file>external_file</file>
  <name>External File</name>
  <description>Downloads external files and makes them available in CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/external-file</url>
    <url desc="Documentation">https://github.com/systopia/external-file</url>
    <url desc="Support">https://github.com/systopia/external-file/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-04-25</releaseDate>
  <version>0.1.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.64</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ExternalFile</namespace>
    <format>23.02.1</format>
    <angularModule>crmExternalFile</angularModule>
  </civix>
  <mixins>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_ExternalFile_Upgrader</upgrader>
</extension>
